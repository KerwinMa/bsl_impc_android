<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<!--项目标识 -->
	<groupId>com.foreveross.chameleon</groupId>
	<artifactId>android</artifactId>
	<version>1.0</version>
	<packaging>pom</packaging>
	<repositories>
	    <repository>
			<id>dayatang-releases</id>
			<url>http://www.dayatang.com/nexus/content/groups/public-release/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>
	<!--配置android依赖 -->
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-install-plugin</artifactId>
						<configuration>
							<groupId>com.foreveross.chameleon.android</groupId>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
						</configuration>
						<executions>
							<execution>
								<id>install-jar1</id>
								<goals>
									<goal>install-file</goal>
								</goals>
								<phase>validate</phase>
								<configuration>
									<artifactId>mina-core</artifactId>
									<version>2.0.7</version>
									<file>${basedir}/libs/mina-core-2.0.7.jar</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jar2</id>
								<goals>
									<goal>install-file</goal>
								</goals>
								<phase>validate</phase>
								<configuration>
									<artifactId>asmack-level7</artifactId>
									<version>3.1.1</version>
									<file>${basedir}/libs/asmack-level7-3.1.1.jar</file>
								</configuration>
							</execution>
<!-- 							<execution>
								<id>install-jar3</id>
								<goals>
									<goal>install-file</goal>
								</goals>
								<phase>validate</phase>
								<configuration>
									<artifactId>cordova</artifactId>
									<version>3.0.0</version>
									<file>${basedir}/libs/cordova-3.0.0.jar</file>
								</configuration>
							</execution> -->
							<execution>
								<id>install-jar4</id>
								<goals>
									<goal>install-file</goal>
								</goals>
								<phase>validate</phase>
								<configuration>
									<artifactId>gson</artifactId>
									<version>2.2.1</version>
									<file>${basedir}/libs/gson-2.2.1.jar</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jar5</id>
								<goals>
